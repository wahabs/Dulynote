<!DOCTYPE html>
<html>
<head>
  <title>Dulynote</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= link_to "Home", root_url %><br><br>
  <% if current_user %>
    <%= button_to "Log Out", session_url, method: :delete %>
  <% else %>
    <%= link_to "Log In", new_session_url %> or <%= link_to "Sign Up", new_user_url %>
  <% end %>

<%= yield %>

</body>
</html>
